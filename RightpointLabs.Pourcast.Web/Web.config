    <mailSettings>
      <smtp deliveryMethod="SpecifiedPickupDirectory" from="pourcast@rightpoint.com">
        <specifiedPickupDirectory pickupDirectoryLocation="c:\temp\maildrop\" />
      </smtp>
    </mailSettings>
